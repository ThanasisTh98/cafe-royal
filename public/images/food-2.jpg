<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#F7F6F1"/>
  
  <!-- Plate -->
  <ellipse cx="200" cy="220" rx="140" ry="25" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="2"/>
  
  <!-- Sandwich base -->
  <rect x="150" y="140" width="100" height="60" rx="8" fill="#D2691E"/>
  <rect x="155" y="145" width="90" height="50" rx="5" fill="#F4A460"/>
  
  <!-- Filling layers -->
  <rect x="160" y="160" width="80" height="4" fill="#228B22"/> <!-- Lettuce -->
  <rect x="160" y="165" width="80" height="6" fill="#FF6347"/> <!-- Tomato -->
  <rect x="160" y="172" width="80" height="8" fill="#8B4513"/> <!-- Meat -->
  <rect x="160" y="181" width="80" height="4" fill="#FFD700"/> <!-- Cheese -->
  
  <!-- Top bread -->
  <rect x="150" y="130" width="100" height="15" rx="8" fill="#D2691E"/>
  
  <!-- Chips/Fries -->
  <g>
    <rect x="100" y="180" width="6" height="25" rx="2" fill="#FFD700"/>
    <rect x="110" y="175" width="6" height="30" rx="2" fill="#FFD700"/>
    <rect x="120" y="185" width="6" height="20" rx="2" fill="#FFD700"/>
    <rect x="130" y="180" width="6" height="25" rx="2" fill="#FFD700"/>
    <rect x="270" y="180" width="6" height="25" rx="2" fill="#FFD700"/>
    <rect x="280" y="175" width="6" height="30" rx="2" fill="#FFD700"/>
    <rect x="290" y="185" width="6" height="20" rx="2" fill="#FFD700"/>
    <rect x="300" y="180" width="6" height="25" rx="2" fill="#FFD700"/>
  </g>
  
  <!-- Salad garnish -->
  <g>
    <circle cx="320" cy="160" r="8" fill="#228B22"/>
    <circle cx="325" cy="170" r="6" fill="#228B22"/>
    <circle cx="315" cy="175" r="7" fill="#228B22"/>
  </g>
  
  <!-- Title -->
  <text x="200" y="50" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="20" font-weight="bold" fill="#C51718">Gourmet Sandwich & Chips</text>
</svg>