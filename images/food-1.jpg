<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#F7F6F1"/>
  
  <!-- Plate -->
  <ellipse cx="200" cy="200" rx="150" ry="30" fill="#FFFFFF" stroke="#E0E0E0" stroke-width="2"/>
  <ellipse cx="200" cy="190" rx="150" ry="10" fill="#F5F5F5"/>
  
  <!-- Food items -->
  <!-- Sausages -->
  <ellipse cx="150" cy="170" rx="40" ry="8" fill="#8B4513"/>
  <ellipse cx="250" cy="170" rx="40" ry="8" fill="#8B4513"/>
  
  <!-- Eggs -->
  <ellipse cx="200" cy="150" rx="25" ry="20" fill="#FFFFFF"/>
  <circle cx="200" cy="150" r="8" fill="#FFD700"/>
  
  <!-- Beans -->
  <g>
    <circle cx="120" cy="190" r="4" fill="#C51718"/>
    <circle cx="130" cy="185" r="4" fill="#C51718"/>
    <circle cx="125" cy="195" r="4" fill="#C51718"/>
    <circle cx="135" cy="190" r="4" fill="#C51718"/>
    <circle cx="115" cy="185" r="4" fill="#C51718"/>
  </g>
  
  <!-- Toast -->
  <rect x="280" y="160" width="30" height="30" rx="5" fill="#D2691E"/>
  <rect x="285" y="165" width="20" height="20" rx="3" fill="#F4A460"/>
  
  <!-- Steam -->
  <g opacity="0.6">
    <path d="M 180 130 Q 175 120 185 110" stroke="#C0C0C0" stroke-width="2" fill="none"/>
    <path d="M 200 130 Q 195 120 205 110" stroke="#C0C0C0" stroke-width="2" fill="none"/>
    <path d="M 220 130 Q 215 120 225 110" stroke="#C0C0C0" stroke-width="2" fill="none"/>
  </g>
  
  <!-- Title -->
  <text x="200" y="50" text-anchor="middle" font-family="Arial, sans-serif" 
        font-size="24" font-weight="bold" fill="#C51718">Full English Breakfast</text>
</svg>